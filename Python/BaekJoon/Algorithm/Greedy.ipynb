   "source": [
    "# 5585 거스름돈\n",
    "\n",
    "n = 1000 - int(input())\n",
    "lst = [500, 100, 50, 10, 5, 1]\n",
    "cnt = 0\n",
    "\n",
    "for i in lst:\n",
    "    cnt += n // i\n",
    "    n %= i\n",
    "    \n",
    "print(cnt)"
   ]
